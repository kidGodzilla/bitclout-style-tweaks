{
  "name": "BitClout Designer Mode",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Enhances the BitClout User Experience",
  "homepage_url": "https://bitclout.com/u/kidgodzilla",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "https://bitclout.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://bitclout.com/*"
      ],
      "css": [
        "src/inject/inject.css"
      ]
    }
  ]
}